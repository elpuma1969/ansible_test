---
-  name: SVI
   gather_facts: no
   hosts: switches
   connection: network_cli

   tasks:
     - name: Configure mult Vlan
       ios_config:
         save_when: modified
         lines:
           - "vlan 75,75,77,78,79,80"
           - ip default-gateway 10.216.75.1

       register: output

     - name: Print vlan config
       debug: vay=output
